{
	"name": "@dreamy-ui/react",
	"version": "1.0.2",
	"description": "Dreamy UI library for React",
	"type": "module",
	"keywords": ["react", "ui", "css-in-js", "component-library"],
	"author": "imexoodeex",
	"license": "MIT",
	"scripts": {
		"build": "tsup",
		"build:dev": "tsup",
		"build:production": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"storybook": "storybook dev -p 6006"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dreamy-ui/dreamy-ui",
		"directory": "packages/react"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./rsc": {
			"types": "./dist/rsc.d.ts",
			"import": "./dist/rsc.js",
			"require": "./dist/rsc.cjs"
		}
	},
	"files": ["/dist"],
	"devDependencies": {
		"@chromatic-com/storybook": "3.2.2",
		"@storybook/addon-a11y": "^8.4.7",
		"@storybook/addon-essentials": "^8.4.7",
		"@storybook/addon-interactions": "8.4.7",
		"@storybook/addon-onboarding": "8.4.7",
		"@storybook/addon-themes": "^8.4.7",
		"@storybook/blocks": "8.4.7",
		"@storybook/react": "^8.4.7",
		"@storybook/react-vite": "^8.4.7",
		"@storybook/test": "8.4.7",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"storybook": "^8.4.7",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vite": "^6.0.3",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"@react-aria/focus": "^3.19.0",
		"@zag-js/focus-visible": "^0.78.3",
		"aria-hidden": "^1.2.4",
		"framesync": "^6.1.2",
		"motion": "^12.23.22",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-focus-lock": "^2.13.5",
		"react-icons": "^5.4.0",
		"react-remove-scroll": "^2.6.2",
		"react-textarea-autosize": "^8.5.6"
	}
}
