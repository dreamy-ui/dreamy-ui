{
	"name": "@dreamy-ui/cli",
	"version": "1.0.0",
	"description": "CLI for Dreamy UI",
	"author": "imexoodeex",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"bin"
	],
	"bin": {
		"dreamy": "./bin/index.js"
	},
	"exports": {
		".": {
			"source": "./src/index.ts",
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"dev": "tsup --watch",
		"start": "bun run ./src/index.ts",
		"lint": "biome lint ./src",
		"build": "tsup"
	},
	"dependencies": {
		"@clack/prompts": "^0.10.1",
		"@effect/platform": "0.69.28",
		"@visulima/boxen": "^1.0.30",
		"cli-table": "^0.3.11",
		"commander": "^13.1.0",
		"debug": "^4.4.0",
		"fs-extra": "11.2.0",
		"globby": "14.0.2",
		"https-proxy-agent": "^7.0.6",
		"look-it-up": "^2.1.0",
		"node-fetch": "^3.3.2",
		"package-manager-detector": "^1.2.0",
		"picocolors": "^1.1.1",
		"pkg-dir": "8.0.0",
		"sucrase": "^3.35.0",
		"yargs": "17.7.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/cli-table": "^0.3.4",
		"@types/debug": "^4.1.12",
		"@types/fs-extra": "11.0.4",
		"@types/node": "22.10.1",
		"@types/yargs": "17.0.33",
		"tsup": "^8.4.0",
		"typescript": "5.7.2"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	}
}
